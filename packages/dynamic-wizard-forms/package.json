{
  "name": "dynamic-wizard-forms",
  "version": "1.0.0",
  "description": "A flexible, configurable wizard form library with conditional navigation for Express.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "wizard",
    "forms",
    "conditional",
    "navigation",
    "express",
    "nunjucks",
    "govuk",
    "typescript",
    "multi-step"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dynamic-wizard-forms.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/dynamic-wizard-forms/issues"
  },
  "homepage": "https://github.com/yourusername/dynamic-wizard-forms#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "example": "cd examples/basic && npm start"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "nunjucks": "^3.2.0"
  },
  "dependencies": {
    "express-session": "^1.17.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.17.0",
    "@types/nunjucks": "^3.2.0",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "rollup": "^3.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "rollup-plugin-dts": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.test.ts"]
  }
}